$('document').ready(function() {
  $('.enabled-handler').click(function() {
    $('.discount-box').toggle();
    $('.discount-checkbox').toggle();
  });
  $('#show_bleed').click(function() {
    $('.bleed').toggle();
  });
  $('#show_safe_area').click(function() {
    $('.safe-area').toggle();
  });
  $('.discount-field').keyup(function() {
    var discount = $(this).val();
    $('.box .wrap').text(discount);
  })
})
