<%= tag.div id: "automation-editor",
            data: {
                id: automation.id,
                return_address: return_address.to_json(only: [:id, :name, :address_line1, :address_line2, :city, :state, :zip, :country_code]),
                automation: automation.to_json(only: [:id, :type, :discount_pct, :discount_exp, :enabled, :international, :send_delay, :front_json, :back_json, :filters, :budget_type, :budget, :budget_update, :credits, :campaign_type, :filter_data, :send_date_start, :send_date_end, :limit_cards_per_day, :campaign_name, :send_continuously, :campaign_status, :created_at, :updated_at, :imported_customers_url]),
                filters: automation.filters.to_json(only: [:id, :filter_data]),
                aws_sign_endpoint: aws_sign_endpoint,
                back_url: _back_url,
                is_user_has_payment_method: @current_shop.current_subscription.present?,
                current_shop: @current_shop
            }
%>